{
    "name": "derekmd/laravel-dusk-firefox",
    "description": "Run Laravel Dusk tests in a Firefox browser",
    "keywords": [
        "dusk",
        "firefox",
        "geckodriver",
        "laravel"
    ],
    "homepage": "https://github.com/derekmd/laravel-dusk-firefox",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/derekmd/laravel-dusk-firefox/issues",
        "source": "https://github.com/derekmd/laravel-dusk-firefox"
    },
    "authors": [
        {
            "name": "Derek MacDonald",
            "email": "derekmd@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/dusk": "^8.0",
        "php-webdriver/webdriver": "^1.15.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^8.19|^9.0",
        "phpunit/phpunit": "^10.1|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Derekmd\\Dusk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Derekmd\\Dusk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "download": "vendor/bin/phpunit tests/DownloadBinaries.php",
        "lint": "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots --diff",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Derekmd\\Dusk\\DuskServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
